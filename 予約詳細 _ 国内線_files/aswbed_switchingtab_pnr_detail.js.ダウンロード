/**
 * @fileoverview 【実行文】「予約詳細」（00010）で「フライト」「搭乗者」「サービス」のタブを切り替える処理
 *
 * @require  jQuery1.8.3
 * @require  aswbed_switchingtab_utility.js
 * @version  0.01
 * @update   2015/2/12
 */
$(function () {
    'use strict';
    
    Aswbed.switchingTabSetting("#tabmenuStatusData", ['#buttonFlight', '#buttonPassenger', '#buttonService'], [['#tabFlight'], ['#tabPassenger'], ['#tabService']]);
});